{
  "name": "nomnoms",
  "version": "1.0.0",
  "engines": {
    "node": "7.5.0",
    "npm": "4.1.2"
  },
  "scripts": {
    "start": "bnr express:prod",
    "dev": "npm-run-all --parallel mongo express:dev webpack browser-sync",
    "mongo": "service mongod start",
    "express:dev": "bnr express:dev",
    "webpack": "webpack",
    "browser-sync": "babel-register browser-sync",
    "build": "bnr build",
    "express:prod": "bnr express:prod"
  },
  "betterScripts": {
    "express:dev": {
      "command": "babel-register index",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "express:prod": {
      "command": "babel-register index",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --optimize-minimize --optimize-occurrence-order",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "browser-sync": "2.18.2",
    "css-loader": "0.26.1",
    "eslint": "3.11.1",
    "extract-text-webpack-plugin": "1.0.1",
    "npm-run-all": "3.1.2",
    "onchange": "3.2.0",
    "postcss-assets": "4.1.0",
    "postcss-cli": "2.6.0",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.2.0",
    "react-hot-loader": "1.3.1",
    "style-loader": "0.13.1",
    "stylus-loader": "2.4.0",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.2"
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-resolver": "1.1.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register-cli": "2.0.1",
    "bcrypt": "1.0.0",
    "better-npm-run": "0.0.13",
    "body-parser": "1.15.2",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "css-modules-require-hook": "4.0.5",
    "express-session": "1.14.2",
    "express": "4.14.0",
    "json-loader": "0.5.4",
    "moment": "2.17.1",
    "mongoose": "4.7.1",
    "multer": "1.2.0",
    "passport-json": "1.2.0",
    "passport": "0.3.2",
    "react-css-modules": "4.1.0",
    "react-dom": "15.4.1",
